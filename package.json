{
  "name": "qqchannel-bot-admin",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev:client": "vite",
    "build:client": "rimraf dist/client/* && vue-tsc --noEmit && vite build",
    "preview:client": "vite preview",
    "dev:server": "cross-env NODE_ENV=development npx nodemon --watch server server/index.ts",
    "build:server": "rimraf dist/server/* && rimraf plugins/* && ncc build ./server/index.ts -o dist/server",
    "build": "npm run build:client && npm run build:server",
    "preview": "node dist/bundle.js",
    "start": "pm2 start dist/bundle.js --name qqchannel-bot",
    "stop": "pm2 stop qqchannel-bot",
    "test": "jest"
  },
