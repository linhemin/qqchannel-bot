{
  "name": "qqchannel-bot-admin",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev:client": "vite",
    "build:client": "rimraf dist/client/* && vue-tsc --noEmit && vite build",
    "preview:client": "vite preview",
    "dev:server": "cross-env NODE_ENV=development npx nodemon --watch server server/index.ts",
    "clean:server": "rimraf dist/server/* && rimraf plugins/*",
    "build:server": "npm run clean:server && tsc --project ./server/tsconfig.json --noEmit && esbuild ./server/index.ts --outfile=dist/server/index.js --bundle --platform=node --target=node12 && node scripts/fix-node12.js && node scripts/copy-plugins.js",
    "build": "npm run build:client && npm run build:server",
    "preview": "node dist/bundle.js",
    "start": "pm2 start dist/bundle.js --name qqchannel-bot",
    "stop": "pm2 stop qqchannel-bot",
    "test": "jest"
  },
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.3.2",
    "@heroicons/vue": "^2.0.11",
    "@paotuan/adapter-kook": "4.5.2-1",
    "@paotuan/adapter-qq": "4.5.1-4",
    "@paotuan/api2d": "0.1.41",
    "@satorijs/satori": "3.6.3",
    "@vuepic/vue-datepicker": "^5.4.0",
    "@vueuse/core": "^9.13.0",
    "chalk": "4",
    "daisyui": "^2.51.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "fast-glob": "^3.3.1",
    "form-data": "^4.0.0",
    "hotkeys-js": "^3.12.0",
    "konva": "^8.3.14",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.0",
    "md5": "^2.3.0",
    "mitt": "^3.0.0",
    "mobx": "^6.6.2",
    "mustache": "^4.2.0",
    "nanoid": "^4.0.0",
    "node-fetch": "^2.6.7",
    "pinia": "^2.0.22",
    "public-ip": "^4.0.4",
    "qq-guild-bot": "^2.9.5",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.37",
    "vue-konva": "^3.0.2",
    "winston": "^3.11.0",
    "ws": "^8.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.189",
    "@types/md5": "^2.3.5",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.7.17",
    "@types/node-fetch": "^2.6.2",
    "@types/sortablejs": "^1.13.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.10",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "esbuild": "^0.20.2",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.3",
