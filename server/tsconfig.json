{
  "compilerOptions": {
    "target": "es2019",
    "module": "ESNext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true,
    "esModuleInterop": true,
    "outDir": "../dist/server",
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
