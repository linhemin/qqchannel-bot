{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "lib": ["ESNext"],
    "declaration": true,
    "emitDeclarationOnly": false,
    "incremental": true,
    "skipLibCheck": true,
    "strictBindCallApply": true,
    "outDir": "lib",
    "rootDir": "src"
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}
